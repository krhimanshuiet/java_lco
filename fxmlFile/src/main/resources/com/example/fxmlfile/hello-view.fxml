    <Button fx:id="Thor"  text="Thor" onAction="#onButtonTapped" GridPane.rowIndex="1"   GridPane.columnIndex="1"  />
    <TextField  fx:id="textFieldOne" GridPane.rowIndex="2" GridPane.columnIndex="0"/>

